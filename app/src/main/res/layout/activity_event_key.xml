<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.hulk.events.Event_key"
    android:background="#a1ffa8"
    android:id="@+id/eventss">

    <GridLayout
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:id="@+id/gridLayout2"
        android:background="@drawable/mic2"></GridLayout>
    <GridLayout
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/agenda"
        android:layout_alignTop="@+id/gridLayout3"
        android:layout_alignLeft="@+id/gridLayout4"
        android:layout_alignStart="@+id/gridLayout4"
        android:background="@drawable/info"></GridLayout>
    <GridLayout
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_above="@+id/gridLayout3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:id="@+id/gridLayout4"
        android:background="@drawable/agenda"></GridLayout>
    <GridLayout
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_marginTop="52dp"
        android:id="@+id/gridLayout3"
        android:layout_below="@+id/gridLayout2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="@drawable/twitter"></GridLayout>
</RelativeLayout>
